<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yantao.dao.RoleMapper">
    <select id="selectRoleById" parameterType="java.lang.String" resultType="com.yantao.pojo.Role">
        select *
        from role
        where rid = #{id,jdbcType=INTEGER};
    </select>
</mapper>